{
  "rules": {
    ".read": false,
    ".write": false,
    ".info": {
      "connected": {
        ".read": true,
        ".write": false
      }
    },
    "status": {
      "$userId": {
        ".read": "auth != null",
        ".write": "auth != null && auth.uid == $userId"
      }
    }
  }
}












